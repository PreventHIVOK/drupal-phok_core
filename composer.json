{
    "name": "drupal/phok_core",
    "type": "drupal-module",
    "description": "The core module of PHOK Platform modules.",
    "keywords": [
        "Drupal 8",
        "PHOK Platform"
    ],
    "license": "GPL-2.0+",
    "homepage": "https://github.com/PreventHIVOK/drupal-phok_core",
    "support": {
        "issues": "https://github.com/PreventHIVOK/drupal-phok_core/issues",
        "source": "https://github.com/PreventHIVOK/drupal-phok_core"
    },
    "require": {
        "drupal/core": "^8.2",
        "adminimal_admin_toolbar": "^1.3",
        "drupal/better_formats": "^1.*@dev",
        "drupal/metatag": "~1.0",
        "drupal/pathauto": "~1.0",
        "drupal/module_filter": "^3.0"
    },
    "require-dev": {
        "mikey179/vfsStream": "~1",
        "phpunit/phpunit": "^4.8 | <6.0"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
